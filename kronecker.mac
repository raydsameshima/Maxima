kill(all);
A: matrix([a1,1],[0,a2]);
B: matrix([1,b1],[b2,0]);
KAB: kronecker_product(A,B);
determinant(KAB);
mat_trace(KAB);
kronecker_product(A,B)-kronecker_product(B,A);

